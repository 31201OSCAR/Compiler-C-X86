prog: lexico.l sintatico.y funcoes.c list.c
	bison -d sintatico.y
	mv sintatico.tab.h sintatico.h
	mv sintatico.tab.c sintatico.c
	flex lexico.l
	mv lex.yy.c lexico.c
	g++ -o prog sintatico.c lexico.c funcoes.c list.c stack.c -I. -lm

clean:
	rm lexico.c sintatico.c sintatico.h programa.asm prog *~
